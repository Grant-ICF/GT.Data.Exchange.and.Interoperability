{
  "@context": {
    "@vocab": "{{ vocab_uri }}",
    {% for key, value in context.items() if key != "@vocab" %}
    "{{ key }}": "{{ value }}"{% if not loop.last %},{% endif %}
    {% endfor %}
  },
  "@id": "{{ spec.jsonld_context_url }}",
  "comment": "Generated JSON-LD context from {{ spec.title }} v{{ spec.version }}"
}