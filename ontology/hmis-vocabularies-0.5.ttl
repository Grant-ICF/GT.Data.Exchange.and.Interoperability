#################################################################
#    3.06.1 Gender Identity
#################################################################

hmis:GenderScheme a skos:ConceptScheme ;
    dcterms:identifier "3.06.1" ;
    dcterms:title "Gender Identity"@en ;
    dcterms:description "Gender identity of clients served in HMIS projects"@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> ;
    dcterms:created "2026-01-28"^^xsd:date ;
    dcterms:modified "2026-01-28"^^xsd:date ;
    dcterms:publisher "U.S. Department of Housing and Urban Development"@en ;
    skos:prefLabel "Gender Identity"@en ;
    skos:definition "Record the gender identity of each client. If a person prefers not to provide a response or says they don't know, do not select any response other than 'Client doesn't know' or 'Client prefers not to answer' on the person's behalf."@en ;
    skos:hasTopConcept hmis:Gender-0, hmis:Gender-1, hmis:Gender-2, hmis:Gender-3, hmis:Gender-4, hmis:Gender-5, hmis:Gender-6, hmis:Gender-8, hmis:Gender-9, hmis:Gender-99 .

hmis:Gender-0 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "0" ;
    skos:prefLabel "Female"@en ;
    skos:definition "A person whose gender identity is female. Gender identity is distinct from sex assigned at birth."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-1 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "1" ;
    skos:prefLabel "Male"@en ;
    skos:definition "A person whose gender identity is male. Gender identity is distinct from sex assigned at birth."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-2 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "2" ;
    skos:prefLabel "Transgender"@en ;
    skos:definition "A person whose gender identity is different from the sex they were assigned at birth."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-3 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "3" ;
    skos:prefLabel "Questioning"@en ;
    skos:definition "A person who is unsure of or exploring their gender identity."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-4 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "4" ;
    skos:prefLabel "Non-Binary"@en ;
    skos:definition "A person whose gender identity is neither exclusively male nor female."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-5 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "5" ;
    skos:prefLabel "Culturally Specific"@en ;
    skos:altLabel "Two-Spirit"@en ;
    skos:definition "A person whose gender identity is specific to their cultural tradition. Examples include Two-Spirit (Native American/Indigenous cultures), Hijra (South Asian cultures), or other culturally specific gender identities."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-6 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "6" ;
    skos:prefLabel "Different Identity"@en ;
    skos:definition "A person whose gender identity is not represented by the other categories provided."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=68> .

hmis:Gender-8 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "8" ;
    skos:prefLabel "Client doesn't know"@en ;
    skos:definition "The client does not know their gender identity."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category - use only when client explicitly states they don't know."@en .

hmis:Gender-9 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "9" ;
    skos:prefLabel "Client prefers not to answer"@en ;
    skos:definition "The client knows their gender identity but prefers not to disclose it."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category - client must explicitly decline to answer; staff cannot select on their behalf."@en .

hmis:Gender-99 a skos:Concept ;
    skos:inScheme hmis:GenderScheme ;
    skos:topConceptOf hmis:GenderScheme ;
    skos:notation "99" ;
    skos:prefLabel "Data not collected"@en ;
    skos:definition "The gender identity information was not collected during the enrollment or data collection process."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category - indicates the worker did not record the information, not that the client declined."@en .

#################################################################
#    2.02.6 Project Type
#################################################################

hmis:ProjectTypeScheme a skos:ConceptScheme ;
    dcterms:identifier "2.02.6" ;
    dcterms:title "HUD Project Types"@en ;
    dcterms:description "Classification of homeless assistance project types as defined by HUD"@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    dcterms:created "2026-01-28"^^xsd:date ;
    dcterms:publisher "U.S. Department of Housing and Urban Development"@en ;
    skos:prefLabel "Project Types"@en ;
    skos:definition "The primary type of project as defined by federal funding source and HUD program regulations."@en ;
    skos:hasTopConcept hmis:ProjectType-1, hmis:ProjectType-2, hmis:ProjectType-3, hmis:ProjectType-4, 
                       hmis:ProjectType-6, hmis:ProjectType-7, hmis:ProjectType-8, hmis:ProjectType-9,
                       hmis:ProjectType-10, hmis:ProjectType-11, hmis:ProjectType-12, hmis:ProjectType-13, hmis:ProjectType-14 .

hmis:ProjectType-1 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "1" ;
    skos:prefLabel "Emergency Shelter - Entry Exit"@en ;
    skos:altLabel "ES - Entry Exit"@en ;
    skos:definition "Emergency shelter projects where clients stay for a short period with a defined entry and exit. Provides temporary shelter with supportive services."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Clients have specific enrollment and exit dates. Typical stay duration tracked for planning purposes."@en .

hmis:ProjectType-2 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "2" ;
    skos:prefLabel "Transitional Housing"@en ;
    skos:altLabel "TH"@en ;
    skos:definition "Housing provided for up to 24 months with supportive services to help individuals and families experiencing homelessness move into permanent housing."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Time-limited housing with intensive services. Maximum 24-month stay under HUD regulations."@en .

hmis:ProjectType-3 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "3" ;
    skos:prefLabel "Permanent Supportive Housing"@en ;
    skos:altLabel "PSH"@en, "PH - Permanent Supportive Housing"@en ;
    skos:definition "Permanent housing with indefinite leasing or rental assistance and supportive services for persons with disabilities experiencing chronic homelessness."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "No time limit on assistance. Disability required for entry. Designed for chronically homeless individuals and families."@en .

hmis:ProjectType-4 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "4" ;
    skos:prefLabel "Street Outreach"@en ;
    skos:altLabel "SO"@en ;
    skos:definition "Essential services provided to unsheltered persons to help them move into emergency shelter, transitional housing, or permanent housing."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Services provided in places not meant for human habitation. Focus on initial engagement and connection to shelter/housing."@en .

hmis:ProjectType-6 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "6" ;
    skos:prefLabel "Services Only"@en ;
    skos:definition "Projects providing supportive services without housing or shelter. May include case management, employment assistance, or other services."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "No housing or shelter component. Services may be provided to persons in any living situation."@en .

hmis:ProjectType-7 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "7" ;
    skos:prefLabel "Other"@en ;
    skos:definition "Projects that do not fit other HUD-defined project type categories."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Use sparingly. Should have clear justification for why project doesn't fit standard types."@en .

hmis:ProjectType-8 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "8" ;
    skos:prefLabel "Safe Haven"@en ;
    skos:altLabel "SH"@en ;
    skos:definition "Supportive housing serving hard-to-reach persons experiencing homelessness with severe mental illness who are unwilling or unable to participate in traditional supportive services."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Low-barrier, harm-reduction approach. Privacy and limited requirements for participation."@en .

hmis:ProjectType-9 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "9" ;
    skos:prefLabel "PH - Housing Only"@en ;
    skos:altLabel "Permanent Housing - Housing Only"@en ;
    skos:definition "Permanent housing with rental assistance but without required supportive services."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Tenant-based rental assistance. Services available but not required."@en .

hmis:ProjectType-10 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "10" ;
    skos:prefLabel "PH - Housing with Services"@en ;
    skos:altLabel "Permanent Housing with Services"@en ;
    skos:definition "Permanent housing with rental assistance and supportive services, but disability not required for entry."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Differs from PSH (ProjectType-3) - no disability requirement. Services are provided but may not be mandatory."@en .

hmis:ProjectType-11 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "11" ;
    skos:prefLabel "Day Shelter"@en ;
    skos:definition "Facility providing daytime shelter and supportive services to persons experiencing homelessness. Does not provide overnight accommodations."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Daytime services only. May provide meals, showers, laundry, case management during daytime hours."@en .

hmis:ProjectType-12 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "12" ;
    skos:prefLabel "Homelessness Prevention"@en ;
    skos:altLabel "HP"@en ;
    skos:definition "Housing relocation and stabilization services and short- and/or medium-term rental assistance to prevent individuals and families from becoming homeless."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Serves persons at-risk of homelessness, not currently homeless. Prevention focus."@en .

hmis:ProjectType-13 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "13" ;
    skos:prefLabel "PH - Rapid Re-Housing"@en ;
    skos:altLabel "RRH"@en, "Rapid Re-Housing"@en ;
    skos:definition "Housing relocation and stabilization services and short- and/or medium-term rental assistance to help individuals and families move quickly out of homelessness into permanent housing."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Time-limited rental assistance (typically 3-24 months). Focus on rapid housing placement and stabilization."@en .

hmis:ProjectType-14 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "14" ;
    skos:prefLabel "Coordinated Entry"@en ;
    skos:altLabel "CE"@en ;
    skos:definition "Coordinated assessment and referral system covering a defined geographic area. Assesses needs, prioritizes clients, and matches to appropriate housing and service interventions."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "Process-focused project type. May operate multiple access points across the CoC."@en .

hmis:ProjectType-0 a skos:Concept ;
    skos:inScheme hmis:ProjectTypeScheme ;
    skos:topConceptOf hmis:ProjectTypeScheme ;
    skos:notation "0" ;
    skos:prefLabel "Emergency Shelter - Night-by-Night"@en ;
    skos:altLabel "ES - NbN"@en, "NbN"@en ;
    skos:definition "Emergency shelter where clients are enrolled night-by-night with no formal entry or exit. Each night of stay is tracked individually."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=32> ;
    skos:note "High-volume, low-barrier shelters. Entry/exit may be automatic based on bed use. Distinct from Entry-Exit (ProjectType-1)."@en .

#################################################################
#    1.8 Yes/No with Reasons for Missing Data
#################################################################

hmis:YesNoReasonsScheme a skos:ConceptScheme ;
    dcterms:identifier "1.8" ;
    dcterms:title "Yes/No with Reasons for Missing Data"@en ;
    dcterms:description "Standard boolean response with additional options for missing data, used across multiple data elements"@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    dcterms:created "2026-01-28"^^xsd:date ;
    dcterms:publisher "U.S. Department of Housing and Urban Development"@en ;
    skos:prefLabel "Yes/No/Missing"@en ;
    skos:definition "Boolean response pattern with structured missing data categories. Used for veteran status, disabling condition, and other yes/no questions requiring missing data capture."@en ;
    skos:hasTopConcept hmis:YesNoReasons-0, hmis:YesNoReasons-1, hmis:YesNoReasons-8, hmis:YesNoReasons-9, hmis:YesNoReasons-99 .

hmis:YesNoReasons-0 a skos:Concept ;
    skos:inScheme hmis:YesNoReasonsScheme ;
    skos:topConceptOf hmis:YesNoReasonsScheme ;
    skos:notation "0" ;
    skos:prefLabel "No"@en ;
    skos:definition "No, without additional context."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> .

hmis:YesNoReasons-1 a skos:Concept ;
    skos:inScheme hmis:YesNoReasonsScheme ;
    skos:topConceptOf hmis:YesNoReasonsScheme ;
    skos:notation "1" ;
    skos:prefLabel "Yes"@en ;
    skos:definition "Yes, without additional context."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> .

hmis:YesNoReasons-8 a skos:Concept ;
    skos:inScheme hmis:YesNoReasonsScheme ;
    skos:topConceptOf hmis:YesNoReasonsScheme ;
    skos:notation "8" ;
    skos:prefLabel "Client doesn't know"@en ;
    skos:definition "The client does not know the answer to the question."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category. Client explicitly states lack of knowledge."@en .

hmis:YesNoReasons-9 a skos:Concept ;
    skos:inScheme hmis:YesNoReasonsScheme ;
    skos:topConceptOf hmis:YesNoReasonsScheme ;
    skos:notation "9" ;
    skos:prefLabel "Client prefers not to answer"@en ;
    skos:definition "The client knows the information but prefers not to provide it."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category. Staff must not select this option on client's behalf."@en .

hmis:YesNoReasons-99 a skos:Concept ;
    skos:inScheme hmis:YesNoReasonsScheme ;
    skos:topConceptOf hmis:YesNoReasonsScheme ;
    skos:notation "99" ;
    skos:prefLabel "Data not collected"@en ;
    skos:definition "The worker did not record the information. May be because client was unavailable or question was not asked."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=15> ;
    skos:note "Missing data category. Distinct from client-initiated missing responses (8, 9)."@en .

#################################################################
#    3.15.1 Relationship to Head of Household
#################################################################

hmis:RelationshipScheme a skos:ConceptScheme ;
    dcterms:identifier "3.15.1" ;
    dcterms:title "Relationship to Head of Household"@en ;
    dcterms:description "Relationship of household members to the designated head of household"@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> ;
    dcterms:created "2026-01-28"^^xsd:date ;
    dcterms:publisher "U.S. Department of Housing and Urban Development"@en ;
    skos:prefLabel "Relationship to HoH"@en ;
    skos:definition "Specifies the relationship of household members to the Head of Household to facilitate reporting on household composition."@en ;
    skos:hasTopConcept hmis:Relationship-1, hmis:Relationship-2, hmis:Relationship-3, hmis:Relationship-4, hmis:Relationship-5 .

hmis:Relationship-1 a skos:Concept ;
    skos:inScheme hmis:RelationshipScheme ;
    skos:topConceptOf hmis:RelationshipScheme ;
    skos:notation "1" ;
    skos:prefLabel "Self (head of household)"@en ;
    skos:altLabel "Head of Household"@en, "HoH"@en ;
    skos:definition "The person designated as head of household. Each household must have exactly one person with this relationship."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> ;
    skos:note "Required for one and only one member per household. Typically the adult member or the person who initiated services."@en .

hmis:Relationship-2 a skos:Concept ;
    skos:inScheme hmis:RelationshipScheme ;
    skos:topConceptOf hmis:RelationshipScheme ;
    skos:notation "2" ;
    skos:prefLabel "Head of household's child"@en ;
    skos:altLabel "Child"@en ;
    skos:definition "The biological, adopted, or foster child of the head of household."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> .

hmis:Relationship-3 a skos:Concept ;
    skos:inScheme hmis:RelationshipScheme ;
    skos:topConceptOf hmis:RelationshipScheme ;
    skos:notation "3" ;
    skos:prefLabel "Head of household's spouse or partner"@en ;
    skos:altLabel "Spouse"@en, "Partner"@en ;
    skos:definition "The spouse (married) or partner (domestic partnership, significant other) of the head of household."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> .

hmis:Relationship-4 a skos:Concept ;
    skos:inScheme hmis:RelationshipScheme ;
    skos:topConceptOf hmis:RelationshipScheme ;
    skos:notation "4" ;
    skos:prefLabel "Head of household's other relation member"@en ;
    skos:altLabel "Other Relation"@en ;
    skos:definition "Any other family relation to the head of household (e.g., parent, sibling, grandchild, aunt, uncle, cousin)."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> .

hmis:Relationship-5 a skos:Concept ;
    skos:inScheme hmis:RelationshipScheme ;
    skos:topConceptOf hmis:RelationshipScheme ;
    skos:notation "5" ;
    skos:prefLabel "Other: non-relation member"@en ;
    skos:altLabel "Non-Relation"@en ;
    skos:definition "A person with no family relation to the head of household but enrolled as part of the household (e.g., friend, roommate)."@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=81> ;
    skos:note "Used for households of unrelated individuals staying together."@en .

#################################################################
#    3.12 Destination at Exit
#################################################################

hmis:DestinationScheme a skos:ConceptScheme ;
    dcterms:identifier "3.12" ;
    dcterms:title "Destination at Program Exit"@en ;
    dcterms:description "The living situation that the client moved to at program exit"@en ;
    dcterms:source <https://files.hudexchange.info/resources/documents/HMIS-Data-Dictionary-2026.pdf#page=76> ;
    dcterms:created "2026-01-28"^^xsd:date ;
    dcterms:publisher "U.S. Department of Housing and Urban Development"@en ;
    skos:prefLabel "Exit Destination"@en ;
    skos:definition "Record where the client is going when they exit the project. Used to track housing outcomes and system performance."@en ;
    skos:note "See Appendix A - Living Situation Option List in FY2026 Data Standards for complete enumeration."@en ;
    skos:hasTopConcept hmis:Destination-101, hmis:Destination-116, hmis:Destination-118, hmis:Destination-204, 
                       hmis:Destination-205, hmis:Destination-206, hmis:Destination-207, hmis:Destination-225,
                       hmis:Destination-302, hmis:Destination-329, hmis:Destination-314, hmis:Destination-332,
                       hmis:Destination-422, hmis:Destination-435, hmis:Destination-8, hmis:Destination-9, hmis:Destination-17, hmis:Destination-24, hmis:Destination-30, hmis:Destination-37, hmis:Destination-99 .

# Permanent Destinations
hmis:Destination-101 a skos:Concept ;
    skos:inScheme hmis:DestinationScheme ;
